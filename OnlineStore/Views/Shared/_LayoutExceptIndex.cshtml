                        </ul>
                    </div>

                    <div class="col-lg-4 col-sm-6 col-12 pl-2">
                        <div class="widgets-wrap ">
                            <div class="widget-header icontext ">

                                @if (!signInManager.IsSignedIn(User))
                                {
                                    <a href="#" class="icon icon-sm rounded-circle border">
                                        <i class="fa fa-user"></i>
                                    </a>
                                }

                                <div class="text ml-0">
                                    @if (!signInManager.IsSignedIn(User))
                                    {
                                        <span class="text-muted">Welcome!</span>
                                    }
                                    else
                                    {
                                        <span class="text-muted">Welcome Back!</span>
                                    }


                                    <div>
                                        <ul class=" navbar-nav mr-auto ml-0">

                                            @if (signInManager.IsSignedIn(User))
                                            {
                                                <li class=" nav-link">
                                                    <form method="post" asp-action="Logout" asp-controller="account">
                                                        <button type="submit" class="nav-item icon icon-xl btn" style=" color:#292b2c">

                                                            <i class="fa fa-user"></i>   Logout @*@User.Identity.Name*@
                                                        </button>
                                                    </form>
                                                </li>
                                            }
                                            else
                                            {
                                                <li class="nav-link">
                                                    <a asp-controller="account" asp-action="Login" style="font-size:15px">Sign in</a> |
                                                    <a class="nav-item" asp-controller="account" asp-action="register" style="color:#292b2c;font-size:15px"> Register</a>
                                                </li>
                                            }
                                        </ul>

                                    </div>
                                </div>
                            </div>






                            @{
                                int subTotal = 0;
                                int totalProductsinCart = 0;
                                var stringObject4 = HttpContextAccessor.HttpContext.Session.GetString("cart");


                                <div class="widget-header float-md-right" style="margin-right:100px;">
                                    <div class="card">
                                        <div class="dropdown">
                                            @if (stringObject4 != null)
                                            {
                                                var cartList = JsonConvert.DeserializeObject<List<Item>>(stringObject4);
                                                @foreach (Item item in (List<Item>)cartList)
                                                {
                                                    totalProductsinCart = totalProductsinCart + item.Quantity;
                                                }
                                                <a href="#" class="btn dropdown-toggle icon icon-xl" style="border:0px;height:100%" data-toggle="dropdown">Cart <i class="fa fa-shopping-cart"></i></a>
                                                <span class="badge badge-pill badge-danger notify">@totalProductsinCart</span>

                                                <div class="dropdown-menu p-3 dropdown-menu-right" style="min-width:280px;">
                                                    <div class="card-body">



                                                        @foreach (Item item in (List<Item>)cartList)
                                                        {
                                                            subTotal = subTotal + item.Product.DiscountPrice * item.Quantity;
                                                            <figure class="itemside mb-3">
                                                                <div class="aside"><img src="~/Images/@item.Product.Image" class="img-sm border"></div>
                                                                <figcaption class="info align-self-center">
                                                                    <p class="title">@item.Product.Name</p>
                                                                    <a asp-controller="Cart" asp-action="RemoveFromCart" asp-route-productId=@item.Product.Id class="float-right"><i class="fa fa-trash"></i></a>
                                                                    <div class="price">Rs. @item.Product.DiscountPrice (@item.Quantity)</div> <!-- price-wrap.// -->
                                                                </figcaption>
                                                            </figure>
                                                        }



                                                        <div class="price-wrap text-center py-3 border-top">Subtotal: <strong class="h5 price">Rs. @subTotal</strong></div>
                                                        <a asp-controller="Home" asp-action="Checkout" class="btn btn-primary btn-block"> Checkout </a>
                                                    </div> <!-- drowpdown-menu.// -->

                                                </div> <!-- dropdown.// -->
                                            }
                                            else
                                            {
                                                <a href="#" class="btn dropdown-toggle icon icon-xl" style="border:0px;" data-toggle="dropdown">Cart <i class="fa fa-shopping-cart"></i></a>
                                                <span class="badge badge-pill badge-danger notify">@totalProductsinCart</span>
                                                <div class="dropdown-menu p-3 dropdown-menu-right" style="min-width:280px;">
                                                    <div class="card-body">
                                                        <p class="title">
                                                            No Product is added to Cart
                                                        </p>
                                                        <div class="price-wrap text-center py-3 border-top">Subtotal: <strong class="h5 price">Rs. @subTotal</strong></div>
                                                        <a asp-controller="Home" asp-action="Checkout" class="btn btn-primary btn-block"> Checkout </a>
                                                    </div>
                                                </div>
                                            }


                                        </div> <!-- card-body.// -->
                                    </div>
                                </div>
                            }


                        </div> <!-- widgets-wrap.// -->

                    </div> <!-- col.// -->
                </div> <!-- row.// -->
            </div> <!-- container.// -->
        </section> <!-- header-main .// -->
