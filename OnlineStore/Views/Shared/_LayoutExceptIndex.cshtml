                            <li class="list-inline-item nav-link "><a asp-action="FilterByPrice" asp-controller="Home" class="h3 text-decoration-none"> SHOP  </a></li>
                            <li class="list-inline-item nav-link"><a href="#" class="h3 text-decoration-none"> ORDERS  </a></li>
