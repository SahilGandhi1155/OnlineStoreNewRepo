                                                            <a class="nav-item" asp-controller="account" asp-action="register" style="color:#292b2c"> Register</a>
                                                        </li>
                                                    }
                                                </ul>

                                            </div>
                                        </div>
                                    </div>
                                </div> <!-- widgets-wrap.// -->

                    </div> <!-- col.// -->
                </div> <!-- row.// -->
                </div> <!-- container.// -->
        </section> <!-- header-main .// -->
