    @RenderBody()

    <hr />
    <div>
        <footer>
            <div class="row">
                <div class="col-md-12 copyright">

                    <div class="col-md-6 col-sm-6 copyright-left">
                        <p>&copy; @DateTime.Now.Year - OnlineShopingStore. All right reserved.</p>
                    </div>

                    <div class="col-md-6 col-sm-6 copyright-right">

                        <ul class="list-unstyled list-social">
                            <li><a href="#" target="_blank"><i class="fa fa-facebook-square"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-google-plus"></i></a></li>

                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        @if (IsSectionDefined("Scripts"))
        {
            @RenderSection("Scripts", required: true)
        }
    </div>
    <div class="container-fluid " style="background:#f7f7f7;color:#292b2c;">



        <div class="row">

            <div class="col-md-3 col-sm-3 col-xs-3">
                <div class="logo">
                    <h1 class="text-center"><a asp-controller="Home" asp-action="Index"><span style="color:black">Black &</span> <span style="color:blue"> Blue</span></a></h1>
                    <p class="text-center">B&B Inc.</p>
                </div>
            </div>

            <div class="col-md-5 col-sm-5 col-xs-5">
                <div style="margin-top:10px; margin-left:50px">


                    <div class="olSearch fr">
                        <input type="text" name="search" placeholder="Search Product/Category" class="inputComn form-control" />
                        <div class="searchIcon">
                            <button type="submit" class="searchBtn">
                                <img src="~/Images/searchIcon.png" />
                            </button>
                        </div>
                    </div>

                </div>
            </div>



            <nav class="navbar navbar-toggleable navbar-light  bg-faded">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#LoginRegisterNavbar " aria-controls="LoginRegisterNavbar" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse col-md-4 col-sm-4" id="LoginRegisterNavbar">
                    <div class="LayoutTopbuttons">

                        <ul class=" navbar-nav mr-auto ">

                            @if (signInManager.IsSignedIn(User))
                            {
                                <li class=" nav-link">
                                    <form method="post" asp-action="Logout" asp-controller="account">
                                        <button type="submit" class="nav-item" style=" color:#292b2c">

                                            Logout @User.Identity.Name
                                        </button>
                                    </form>
                                </li>
                            }
                            else
                            {
                                <li class="  nav-link">
                                    <a class="nav-item" asp-controller="account" asp-action="register" style="color:#292b2c">Register</a>
                                </li>
                                <li class="  nav-link">

                                    @{
                                        var uriBuilder = new UriBuilder
                                        {
                                            Scheme = ViewContext.HttpContext.Request.Scheme,
                                            Host = ViewContext.HttpContext.Request.Host.ToString(),
                                            Path = ViewContext.HttpContext.Request.Path.ToString()
                                        };

                                        //Uri uri = uriBuilder.Uri;

                                    }
                                    <a class="nav-item" asp-controller="account" asp-action="Login" asp-route-returnurl="@uriBuilder">Login</a>
                                </li>
                            }

                            <li class=" dropdown nav-link">
                                <a class="nav-item dropdown-toggle" data-toggle="dropdown" href="#">Cart <i class="glyphicon glyphicon-shopping-cart">  </i> </a>
                                <ul class=" dropdown-menu">

                                    @{
                                        var stringObject2 = HttpContextAccessor.HttpContext.Session.GetString("cart");

                                        if (stringObject2 != null)
                                        {

                                            var cartList = JsonConvert.DeserializeObject<List<Item>>(stringObject2);
                                            foreach (Item item in (List<Item>)cartList)
                                            {
                                                <li class="list-group-item " style="color:black">
                                                    @item.Product.Name (@item.Quantity)  <a style="color:lightskyblue" asp-controller="Cart" asp-action="RemoveFromCart" asp-route-productId=@item.Product.Id>  <small>Remove</small> <i class="fa fa-times"></i></a>
                                                </li>
                                            }

                                        }
                                    }
                                    <li class="divider"></li>
                                    <li class=" nav-link">
                                        <a class="nav-item" asp-controller="Home" asp-action="Checkout">Checkout</a>
                                    </li>


                                </ul>
                            </li>
                        </ul>

                    </div>
                </div>

                <div class="card">
                    <div class="card-body">
                        <div class="dropdown">
                            <a href="#" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                                Show cart
                            </a>
                            <div class="dropdown-menu p-3 dropdown-menu-right" style="min-width:280px;">
                                @{
                                    int subTotal2 = 0;
                                    var stringObject3 = HttpContextAccessor.HttpContext.Session.GetString("cart");

                                    if (stringObject3 != null)
                                    {

                                        var cartList4 = JsonConvert.DeserializeObject<List<Item>>(stringObject2);
                                        foreach (Item item in (List<Item>)cartList4)
                                        {
                                            subTotal2 = subTotal2 + item.Product.DiscountPrice * item.Quantity;
                                            <figure class="itemside mb-3">
                                                <div class="aside"><img src="~/Images/@item.Product.Image" class="img-sm border"></div>
                                                <figcaption class="info align-self-center">
                                                    <p class="title">@item.Product.Name</p>
                                                    <a asp-controller="Cart" asp-action="RemoveFromCart" asp-route-productId=@item.Product.Id class="float-right"><i class="fa fa-trash"></i></a>
                                                    <div class="price">Rs. @item.Product.DiscountPrice (@item.Quantity)</div> <!-- price-wrap.// -->
                                                </figcaption>
                                            </figure>
                                        }
                                    }
                                }

                                <div class="price-wrap text-center py-3 border-top">Subtotal: <strong class="h5 price">Rs. @subTotal2</strong></div>
                                <a asp-controller="Home" asp-action="Checkout" class="btn btn-primary btn-block"> Checkout </a>
                            </div> <!-- drowpdown-menu.// -->
                        </div>  <!-- dropdown.// -->

                    </div> <!-- card-body.// -->
                </div> <!-- card.// -->
                <!-- ============================ COMPONENT 4 END .// ================================= -->
            </nav>





        </div>
    </div>


</body>
</html>

